{
    "name": "Weather App",
    "description": "Extension for weather app.",
    "version": "1.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "weather.js"
    },
    "action": {
        "default_popup": "index.html",
        "defaut_icon": {
            "16": "/icons/icon16.png",
            "32": "/icons/icon32.png",
            "48": "/icons/icon48.png",
            "128": "/icons/icon128.png"
        }
    },
    "icons": {
        "16": "/icons/icon16.png",
        "32": "/icons/icon32.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
    },
    "permissions": [ "storage" ]
}