{
    "name": "Weather App",
    "description": "Extension for weather app.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "permissions": [ "storage", "activeTab", "scripting" ],
    "action": {
        "default_popup": "index.html",
        "defaut_icon": {
            "16": "/img/icon16.png",
            "32": "/img/icon32.png",
            "48": "/img/icon48.png",
            "128": "/img/icon128.png"
        }
    },
    "icons": {
        "16": "/img/icon16.png",
        "32": "/img/icon32.png",
        "48": "/img/icon48.png",
        "128": "/img/icon128.png"
    }
}